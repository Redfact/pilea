div= document.getElementById("likebutton<%=@selection.coin_id%>")
childs= div.childNodes
for (i = 0; i <childs.length; i++) {
    if(childs[i].localName === 'a'){
        childs[i].remove()
    }
}
div.insertAdjacentHTML("beforeend", "<%= j render 'assets/selection_false', selection: @selection %>")